# Vision Provider Configuration
# Supported providers: openrouter, openai, mock
VISION_PROVIDER=openrouter
VISION_MODEL=
VISION_TIMEOUT=30.0

# OpenRouter API Configuration (when VISION_PROVIDER=openrouter)
